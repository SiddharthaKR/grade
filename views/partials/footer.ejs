function calc(){
  const g=[];
for(let i=0;i<=7;i++){

  let x= document.getElementsByClassName("mySelect")[i].value;
  switch(x){
    case "AA":
    g.push(10);
    break;
    case "AB":
    g.push(9);
    break;
    case "BB":
    g.push(8);
    break;
    case "BC":
    g.push(7);
    break;
    case "CC":
    g.push(6);
    break;
    case "CD":
    g.push(5);
    break;
    case "DD":
    g.push(4);
    break;
    case "F":
    g.push(0);
    break;
    default:
    g.push(0);
  }
}
let spi=(6*g[0]+6*g[1]+3*g[2]+3*g[3]+8*g[4]+8*g[5]+3*g[6]+6*g[7])/(43);
document.querySelector(".result h1").innerText="Your SPI is: "+spi;
}


